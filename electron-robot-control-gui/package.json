{
  "name": "electron-robot-motor-control-gui",
  "productName": "RosRobotControl",
  "version": "0.1.1",
  "description": "Electron GUI to control a 2 wheel car chassis via ROS",
  "license": "MIT",
  "repository": "https://github.com/neumann89/raspi-robot-control",
  "author": {
    "name": "neumann89",
    "url": "github.com/neumann89"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "1.4.15"
  },
  "scripts": {
    "build": "cmake-js rebuild",
    "start": "electron .",
    "release": "electron-packager . $npm_package_productName --platform=linux --arch=x64 --out=dist --app-version=$npm_package_version --prune --asar --overwrite"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.0",
    "angular-aria": "1.5.0",
    "angular-material": "1.0.7",
    "angular-messages": "1.5.0",
    "electron-debug": "0.5.2",
    "nan": "2.5.1"
  },
  "devDependencies": {
    "cmake-js": "3.4.1",
    "electron-packager": "7.7.0",
    "electron": "1.4.15"
  }
}
